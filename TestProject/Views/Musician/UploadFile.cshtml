@model TestProject.Models.FileViewModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@using (Html.BeginForm("UploadFile", "Musician", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="control-group">
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.PostedFile, new { type = "file", @class = "input-xlarge" })
        </div>
    </div>
    <div class="col-md-offset-2 col-md-10">
        <input type="submit" class="btn btn-default" value="Upload File" />
    </div>
}

