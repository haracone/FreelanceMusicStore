@model  TestProject.Models.MusicInstrumentForDropdown

<div class="panel panel-success">
    <div class="panel-body">
        <form class="p-a-1" asp-action="ChangeMusicInstruments" method="post">
            <div class="form-group">
                @Html.LabelFor(m => m.MusicInstrumentId, new { @class = "col-lg-2 control-label" })
                <div class="col-md-10">
                    @Html.DropDownListFor(m => m.MusicInstrumentId, new MultiSelectList(Model.MusicInstuments ,"Id", "Name"), new { @class = "form-control" })
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" class="btn btn-default" value="Change music instruments" />
                </div>
            </div>
        </form>
    </div>
</div>
